   ],
   "source": [
    "# import packages\n",
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import urllib.parse as urlparse\n",
    "import seaborn as sns\n",
    "\n",
    "sns.set(font_scale=1.2, style=\"whitegrid\")\n",
    "\n",
    "# set-up the API request\n",
    "url_base = 'https://data.colorado.gov/resource/ya69-n6ta.json?'\n",
    "api_query = {\n",
    "    'policeDistrict': 'Boulder PD',\n",
    "    '$where': 'year between 2001 and 2016',\n",
    "    '$select': 'year,type,count'\n",
    "}\n",
    "url = url_base + urlparse.urlencode(api_query)\n",
    "url\n",
    "\n",
    "url = url_base + urlparse.urlencode(api_query)\n",
    "url_final = url.replace(\" \", \"%20\")\n",
    "\n",
    "# Read in the data, sorted and indexed by year\n",
    "boulder_crime = pd.read_json(url_final)\n",
    "boulder_crime_final = boulder_crime.dropna()\n",
    "boulder_crime_final\n",
    "boulder_crime_final.set_index('year').sort_index()\n",
    "\n",
    "# Pivot the data for stacked plotting\n",
    "boulder_crime_pivot = (\n",
    "    boulder_crime_final\n",
    "    .pivot_table('count',['year'], 'type')\n",
    ")\n",
    "boulder_crime_pivot\n",
    "\n",
    "# Plot the data\n",
    "f, ax = plt.subplots(figsize=(13, 20))\n",
    "title = 'Reported Crimes in Boulder, CO by Type\\n(2001-2016)'\n",
    "boulder_crime_pivot.plot.bar(ax=ax,\n",
    "                             title=title,\n",
    "                             stacked=True).legend(loc='upper right', \n",
    "                                                  title='Crime Type')\n",
    "\n",
    "ax.set(xlabel=\"Year\",\n",
    "       ylabel=\"Total Reported Cases\")\n",
    "\n",
    "\n",
    "# Saving figure\n",
    "plt.savefig(\"boulder_crime_final.jpg\")"
   ]
  },
